<form method="post" action="{{ path('commercial_entity_delete', {'id': commercial_entity.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ commercial_entity.id) }}">
    <button class="btn">Delete</button>
</form>
